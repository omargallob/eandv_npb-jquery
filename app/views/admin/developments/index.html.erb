<div id="side-nav">
	<ul>
		<%= render :partial => "shared/admin/developments_sidenav" %>
	</ul>
</div>

<div id="main">
	<h1>Development list for:<br/> <strong><%= @location.region %> </strong> (<%= @location.zipcod %>) | <%= @location.county %> County  | <%= @location.state %> | <%= @location.country.title %></h1>
	<table>	
		<tr>
			<td><strong>Title</strong></td>
			<td>&nbsp;|&nbsp;</td>
			<td><strong>Type</strong></td>
			<td>&nbsp;|&nbsp;</td>
			<td><strong>Description</strong></td>
			<td>&nbsp;|&nbsp;</td>
			<td><strong>Options</strong></td>
			
		</tr>
		<% if @location.developments %>
			<%= render :partial => 'development', :collection => @location.developments %>	
		<% end %>
	</table>
</div>