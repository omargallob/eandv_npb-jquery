<div id="main">
	<h1>Editing Development at:<br/> <%= @location.region %> (<%= @location.zipcod %>) | <%= @location.county %> County | <%= @location.state %> | <%= @location.country.title %></h1>

	<% form_for @development, :url => {:action => "update"}  do |f| %>	
	 <%= f.error_messages %> 
	  <%= render :partial => "form", :locals => {:f =>f}%>
	  <p>
	    <%= f.submit 'Create' %>
	  </p>
	<% end %>

	<%= link_to 'Back', admin_types_path %>
</div>