<div id="main">
	<h1>Editing Location in <%= @location.country.title %> </h1>

	<% form_for @location, :url => {:action => "update"}  do |f| %>	
	 <%= f.error_messages %> 
	  <%= render :partial => "form", :locals => {:f =>f}%>
	  <p>
	    <%= f.submit 'Update' %>
	  </p>
	<% end %>

	<%= link_to 'Back', admin_country_path(@location.country.country_initials.downcase) %>
</div>