<div id="main">
	<h1>New Location for <%= @country.title %></h1>

	<% form_for :location, :url => {:action => "create"}  do |f| %>	
	<%= f.error_messages %> 
	  <%= render :partial => "form", :locals => {:f =>f}%>
	  <p>
	    <%= f.submit 'Create' %>
	  </p>
	<% end %>

	<%= link_to 'Back', admin_countries_path %>
</div>