<div id="left-side">
	<div id="title">
		<h5><%= @page.navlabel %></h5>
	</div>
	
	<div id="side-nav">

		<hr />
		<div id="leftswfcontainer" style="float:left;height:196px">
			<%= render :partial =>"shared/keepdiscovering" %>
			<%= render :partial=>"shared/polaroid"%>
		</div>
		<%= image_tag "system/buttons/sellyourproperty.jpg",:style => "margin-top:8px;" %>
		<hr />
		<div class="left_tool relocating">
			<div class="top"></div>
			<div class="header_w_image">
				<img src="/images/system/buttons/affiliate.jpg" width="50" height="35" alt="House">
				<h3 class="services"><span class="title"><a href="/contact?iframe=true&width=560&height=600" rel="prettyPhoto">Become an <br />affiliate</a></span></h3>
			</div>
			<div class="bottom"></div>
		</div>
	</div>

	<div id="main" class="properties">
		<%= image_tag "static/services_main.jpg",:id=>"mainpic" %>
		<%= @page.body %>
		<ol id="blog">
		<%= render :partial => "post", :collection => @posts %>
		</ol>
		<div id="main_bottom" class="main_border other">	</div>
	</div>
	

</div>
<div id="right-column">
	<div id="searchbox" 	class="advanced">
		<div class="top"></div>
			<div><%= render :partial => "shared/advancedsearchbox" %></div>
		<div class='bottom'></div>
	</div>
</div>

<% content_for :script do %>
<script type="text/javascript">
$(document).ready(function() {
  	 

	$('.droppies').combobox(); 

	
	$("#main_bottom").appendTo($("#main"));
	set_nav_pointer("blog");
});


</script>
<% end %>
